{
    "buildCommand": "cd sabai_frontend && npm install && npm run build",
    "outputDirectory": "sabai_frontend/build",
    "builds": [
      {
        "src": "sabai_backend/sabai_backend/wsgi.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "sabai_backend/sabai_backend/wsgi.py"
      },
      {
        "src": "/(.*)",
        "dest": "/sabai_frontend/build/$1"
      }
    ]
  }